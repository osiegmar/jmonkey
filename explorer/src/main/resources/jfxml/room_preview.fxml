<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="120.0"
      prefWidth="160.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="de.siegmar.jmonkey.explorer.controller.RoomPreviewController">
   <ImageView fitHeight="100.0" fitWidth="160.0" pickOnBounds="true" preserveRatio="true" fx:id="roomImage"/>
   <Label layoutY="100.0" prefHeight="20.0" prefWidth="160.0" text="Label" textAlignment="CENTER" fx:id="roomName">
      <padding>
         <Insets left="3.0" right="3.0"/>
      </padding>
   </Label>
</Pane>
